{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "no.geonorge.dokanalyse.v1.output.schema.json",
  "title": "AnalysisOutput",
  "type": "object",
  "properties": {
    "resultList": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "runAlgorithm": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "buffer": {
            "type": "integer"
          },
          "runOnInputGeometry": {
            "oneOf": [
                        {
                          "$ref": "http://geojson.org/schema/Polygon.json"
                        },
                        {
                          "$ref": "http://geojson.org/schema/MultiPolygon.json"
                        }
                      ]
          },
          "runOnDataset": {
            "$ref": "no.geonorge.dokanalyse.v1.dataset.schema.json"
          },
          "inputGeometryArea": {
            "type": "integer"
          },
          "hitArea": {
            "type": "integer"
          },
          "resultStatus": {
            "type": "string",
            "enum": [
              "HIT",
              "NO-HIT",
              "HIT-RED",
              "HIT-YELLOW",
              "NO-HIT-YELLOW",
              "NO-HIT-GREEN",
              "NOT-RELEVANT",
              "TIMEOUT",
              "ERROR"
            ]
          },
          "possibleActions": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "rasterresult": {
            "type": "string",
            "contentEncoding": "base64",
            "contentMediaType": "image/png"
          },
          "cartography": {
            "type": "string",
            "contentEncoding": "base64",
            "contentMediaType": "image/png"
          },
          "guidanceText": {
            "type": "string"
          },
          "guidanceUri": {
            "type": "array",
            "items": {
              "$ref": "no.geonorge.dokanalyse.v1.uri.schema.json"
            }
          },
          "description": {
            "type": "string"
          },
          "distanceToObject": {
            "type": "integer"
          },
          "theme": {
            "type": "string"
          },
          "qualitymeasurement": {
            "type": "array",
            "items": {
              "$ref": "no.geonorge.dokanalyse.v1.qualitymeasurement.schema.json"
            }
          },
          "qualityWarning": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "data": {
            "type": "object"
          }
        },
        "required": [
          "runAlgorithm",
          "runOnDataset",
          "resultStatus"
        ]
      }
    },
    "report": {
      "type": "string",
      "contentEncoding": "base64",
      "contentMediaType": "application/pdf"
    },
    "inputGeometry": {
      "oneOf": [
        {
          "$ref": "http://geojson.org/schema/Polygon.json"
        },
        {
          "$ref": "http://geojson.org/schema/MultiPolygon.json"
        }
      ]
    },
    "inputGeometryArea": {
      "type": "integer"
    },
    "factInfoRasterResult": {
      "type": "string"
    },
    "factInfoCartography": {
      "type": "string"
    },
    "factList": {
      "type": "array",
      "items": {
          "$ref": "no.geonorge.dokanalyse.v1.factpart.schema.json"
        }
    },
    "municipalityNumber": {
      "type": "string"
    },
    "municipalityName": {
      "type": "string"
    }
  }
}